size(200);
pair O = (0, 0);
pair A = (3, 0);
pair B = (0, 3);
pair C = (2.5, 2.5);
pair D = (-1, 2);

draw(O--A, black);
draw(O--B, black);
draw(O--C, black);
draw(O--D, black);

dot(O); label("$O$", O);
dot(A); label("$A$", A);
dot(B); label("$B$", B);
dot(C); label("$C$", C);
dot(D); label("$D$", D);